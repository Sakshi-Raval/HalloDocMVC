                            <div class="col-6 px-3">
                                <a class="btn btn-success text-white border-1 px-3 rounded-pill btn w-100" asp-action="ViewUploads" asp-controller="Admin" asp-route-requestid="@item.RequestId">View Uploads</a>
                            </div>
                            <div class="col-6 px-3">
                                <button class="btn btn-success text-white border-1 rounded-pill btn w-100">Encounter</button>
                            </div>
                            <div class="col-6 px-3">
                                <button class="btn border-1 px-3 rounded-pill btn w-100" style="background-color:#EE9125;color:white" onclick="clearCaseModal(this)" data-Requestid="@item.RequestId">Clear Case</button>
                            </div>
                            <div class="col-6 px-3">
                                <button class="btn border-1 btn-success px-3 rounded-pill btn w-100"> Email</button>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
            i++;
