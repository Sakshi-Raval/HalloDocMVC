                                <button class="btn btn-danger text-white border-1 rounded-pill btn w-100">Close Case</button>
                            </div>
                            <div class="col-6 px-3">
                                <a class="btn border-1 btn-success px-3 rounded-pill btn w-100" asp-action="ViewNotes" asp-controller="Admin" asp-route-requestid="@item.RequestId"> View Notes</a>
                            </div>
                            <div class="col-6 px-3">
                                <button class="btn btn-info text-white border-1 rounded-pill btn w-100" style="background-color:#145545;">Doctors Note</button>
